<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose", method="post">
    <div class="mb-3">
        <label for="disabledTextInput" class="form-label">Title</label>
        <input type="text" name="text" id="disabledTextInput" class="form-control">
      </div>
      <div class="mb-3">
        <label for="disabledSelect" class="form-label">Post</label>
        <div>
            <textarea name="post" id="" cols="160" rows="10"></textarea>
        </div>
      </div>
      <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer") -%>